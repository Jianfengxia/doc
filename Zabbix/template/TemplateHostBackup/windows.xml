			<triggers>
				<trigger>
					<description>Lack of free memory on server {HOSTNAME}</description>
					<expression>{{HOSTNAME}:vm.memory.size[free].last(0)}&lt;10000</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Low free disk space on {HOSTNAME} volume c:</description>
					<expression>{{HOSTNAME}:vfs.fs.size[c:,free].last(0)}&lt;10000</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Too many processes on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:proc.num[].last(0)}&gt;300</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Processor load is too high on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:system.cpu.load[,avg1].last(0)}&gt;5</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Too many processes running on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:system[procrunning].last(0)}&gt;10</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Lack of free swap space on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:system.swap.size[,free].last(0)}&lt;100000</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>c:\autoexec.bat has been changed on server {HOSTNAME}</description>
					<expression>{{HOSTNAME}:vfs.file.cksum[c:\autoexec.bat].diff(0)}&gt;0</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>{HOSTNAME} has just been restarted</description>
					<expression>{{HOSTNAME}:system.uptime.last(0)}&lt;600</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Apache is not running on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:proc_cnt[httpd].last(0)}&lt;1</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Host information was changed on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:system.uname.diff(0)}&gt;0</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Version of zabbix_agent(d) was changed on {HOSTNAME}</description>
					<expression>{{HOSTNAME}:agent.version.diff(0)}&gt;0</expression>
					<priority>1</priority>
				</trigger>
				<trigger>
					<description>Server {HOSTNAME} is unreachable</description>
					<expression>{{HOSTNAME}:status.last(0)}=2</expression>
					<priority>3</priority>
				</trigger>
				<trigger>
					<description>Low free disk space on {HOSTNAME} volume d:</description>
					<expression>{{HOSTNAME}:vfs.fs.size[d:,free].last(0)}&lt;10000</expression>
					<priority>3</priority>
				</trigger>
